<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:controller="com.cloud.client.RegController"
      xmlns:fx="http://javafx.com/fxml" alignment="center" >

    <TextField fx:id="loginField"  promptText="login" />
    <PasswordField fx:id="passwordField"  promptText="password"/>
    <TextField fx:id="nickField"  promptText="nickname" />
    <TextArea fx:id="textArea" VBox.vgrow="ALWAYS"/>
    <Button text="Registration"  minWidth="100" onAction="#tryToReg"/>
</VBox>
